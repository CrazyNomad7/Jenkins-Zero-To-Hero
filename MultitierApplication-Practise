pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'maven:3.8.2-adoptopenjdk-11' }
      }
      steps {

        sh 'mvn clean install'

        sh 'java -cp target/*.jar com.example.HelloWorld'
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {

         sh 'npm install'

        sh 'node app.js'

       
      }
    }
  }
}
